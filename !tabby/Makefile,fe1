# Project:   application


# Toolflags:
CCflags = -c -IRISC_OSLib: -throwback -I<Logger$Dir>
C++flags = -c -IRISC_OSLib: -throwback -I<Logger$Dir>
Linkflags = -aif -c++ -o $@
ObjAsmflags = -throwback -NoCache 
CMHGflags = 
LibFileflags = -c -o $@
Squeezeflags = -o $@
ModSqzflags = -o $@

OBJS = @.o.main @.o.tabby RISC_OSLib:o.risc_oslib C:o.stubs @.o.logger

# Final targets:
@.application: $(OBJS)
        Link $(Linkflags) $(OBJS)


.INIT:
        @CDir o

# User-editable dependencies:

# Static dependencies:

@.o.main:  @.c.main @.h.tabby <Logger$Dir>.h.logger
        cc $(ccflags) -o @.o.main @.c.main 

@.o.tabby:  @.c.tabby @.h.tabby <Logger$Dir>.h.logger
        cc $(ccflags) -o @.o.tabby @.c.tabby 

@.o.logger: <Logger$Dir>.h.logger <Logger$Dir>.c.logger 
        cc $(ccflags) -o @.o.logger <Logger$Dir>.c.logger 
